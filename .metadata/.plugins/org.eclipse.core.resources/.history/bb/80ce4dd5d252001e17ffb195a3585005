package com.class2;



public class TestClass { 
	private void addition() {
		int a=5;
		int b=6;
		int c=a+b;
		System.out.println(c);

	}
	private void subraction() {
		int a=10;
		int b=6;
		int c=a-b;
System.out.println(c);
	}
	private void multiplication() {
		int a= 8
		int b=5
		int c=a*b
		System.out.println(c);

	}
	
public static void main(String[] args) {
	TestClass test = new TestClass();
		test.addition();
		test.subraction();
		test.multiplication();
	
}
}
